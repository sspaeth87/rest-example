<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering SYSTEM "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
    <type-mapping>
        <sql-type jdbc-type="CLOB" hibernate-type="java.lang.String" />
        <sql-type jdbc-type="BLOB" hibernate-type="byte[]" />
    </type-mapping>
    <table-filter match-catalog="NORTHWIND" match-schema="PUBLIC" match-name="CUSTOMERS" />
    <table catalog="NORTHWIND" schema="PUBLIC" name="CUSTOMERS" class="de.xdevsoftware.restexample.domain.Customer" />
</hibernate-reverse-engineering>
